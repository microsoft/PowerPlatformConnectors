                "LastUpdateDateEnd": {
                  "type": "string",
                  "description": "Specify the end of the last update time span",
                  "title": "Last update date (end)",
                  "format": "date-time"
                },
                "AssetStatus": {
                  "type": "integer",
                  "format": "int32",
                  "description": "Specify the asset status",
                  "title": "Asset status"
                },
                "IsDeprecated": {
                  "type": "integer",
                  "description": "Specify whether you would like to receive deprecated assets.",
                  "title": "Is deprecated?",
                  "enum": [
                    0,
                    1
                  ],
                  "format": "int32",
                  "default": 0
                },
                "ResponseType": {
                  "type": "integer",
