        "text": {
          "title": "Text",
          "description": "Text that will be stamped",
          "type": "string",
          "x-ms-summary": "Text",
          "x-ms-visibility": ""
        },
        "options": {
          "type": "object",
          "properties": {
            "color": {
              "title": "Font Color",
              "description": "Color of the font",
              "default": "#000000",
              "type": "string",
              "x-ms-summary": "Font Color",
              "x-ms-visibility": "advanced"
            },
            "size": {
              "title": "Font Size",
              "description": "Size of the font",
              "default": "50",
              "type": "integer",
              "x-ms-summary": "Font Size",
              "x-ms-visibility": "advanced"
            },
            "rotate": {
              "title": "Rotate",
              "description": "Rotate the text with degrees",
              "default": "0",
              "type": "integer",
              "x-ms-summary": "Rotate",
              "x-ms-visibility": "advanced"
            },
            "position": {
              "type": "object",
              "properties": {
                "yOffset": {
                  "title": "Y-Offset",
                  "description": "Y-Offset",
                  "default": "25",
                  "type": "integer",
                  "x-ms-summary": "Y-Offset",
                  "x-ms-visibility": "advanced"
                },
                "yOffsetStart": {
                  "title": "Start of Y-Offset",
                  "description": "Start of Y-Offset bottom or top",
                  "default": "bottom",
                  "type": "string",
                  "x-ms-summary": "Start of Y-Offset",
                  "x-ms-visibility": "advanced"
                },
                "xOffset": {
                  "title": "X-Offset",
                  "description": "X-Offset",
                  "default": "50",
                  "type": "integer",
                  "x-ms-summary": "X-Offset",
                  "x-ms-visibility": "advanced"
                }
              }
            }
          }
        }
      },
      "example": {
        "pdf": "base64",
        "text": "value",
        "options": {
          "color": "#000000",
          "size": 50,
          "rotate": 0,
          "position": {
            "yOffset": 25,
            "yOffsetStart": "bottom",
            "xOffset": 50
          }
        }
      }
    },
