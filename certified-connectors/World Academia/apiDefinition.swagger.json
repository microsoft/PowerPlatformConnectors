                  },
                  "state_province": {
                    "type": "string"
                  },
                  "web_pages": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "No Results"
          },
          "504": {
            "description": "An error occurred"
          }
        },
        "operationId": "Get_all_universities_in_a_country_via_country_name",
        "description": "Get all universities in a country  Media type"
