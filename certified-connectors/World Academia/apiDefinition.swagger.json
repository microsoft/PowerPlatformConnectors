          "200": {
            "description": "Get the number of universities available",
            "schema": {
              "type": "integer"
            }
          },
          "404": {
            "description": "No Results"
          },
          "504": {
            "description": "An error occurred"
          }
        },
        "description": "Get the number of universities available.",
        "operationId": "Get_the_number_of_universities_available",
