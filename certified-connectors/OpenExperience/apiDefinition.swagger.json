  "paths": {
    "/connector/createProject": {
      "post": {
        "responses": {
          "default": {
            "description": "default",
            "schema": {}
          }
        },
        "summary": "Create new project",
        "operationId": "CreateNewProject",
        "parameters": [
          {
            "name": "Project Settings",
            "in": "body",
            "required": false,
            "schema": {
              "type": "object",
              "properties": {
                "customerId": {
                  "type": "string",
                  "description": "customerId",
                  "title": "Customer",
                  "x-ms-dynamic-values": {
                    "operationId": "GetAvailableCustomers",
                    "value-path": "id",
                    "value-collection": "accounts",
                    "value-title": "label"
                  }
                },
                "id": {
                  "type": "string",
                  "description": "id",
                  "title": "Project ID"
                },
                "name": {
                  "type": "string",
                  "description": "name",
                  "title": "Project Name"
                },
                "address": {
                  "type": "string",
                  "description": "address",
                  "title": "Address"
                },
                "contactPhone": {
                  "type": "integer",
                  "format": "int32",
                  "description": "contactPhone",
                  "title": "Contact Phone"
                },
                "contactEmail": {
                  "type": "string",
                  "description": "contactEmail",
                  "title": "Contact Email"
                },
                "responsible": {
                  "type": "string",
                  "description": "responsible",
                  "title": "Responsible Person"
                },
                "services": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "title": "Services",
                    "x-ms-dynamic-values": {
                      "operationId": "GetAvailableServices",
                      "value-path": "type",
                      "value-collection": "services",
                      "value-title": "serviceFullname"
                    },
                    "description": "Services in which project will be created"
                  },
                  "description": "services"
                },
                "settings": {
                  "type": "object",
                  "properties": {
                    "projectAdminMembersAccess": {
                      "type": "boolean",
                      "description": "Set to \"Yes\" if you wan't to allow Project Administrators to be able to edit subcontractors in this project.",
                      "title": "Can Project Admin edit subcontractors?",
                      "enum": [
                        true,
                        false
                      ],
                      "default": false
                    }
                  },
                  "description": "settings",
                  "required": [
                    "projectAdminMembersAccess"
                  ]
                }
              },
              "required": [
                "customerId",
                "services",
                "contactEmail",
                "contactPhone",
                "name",
                "id",
                "responsible",
                "settings"
              ]
            }
          }
        ],
        "description":"Creates new project in Open Experience for selected services"
      }
    },
