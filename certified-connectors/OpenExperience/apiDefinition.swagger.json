              "properties": {
                "accounts": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "description": "id"
                      },
                      "label": {
                        "type": "string",
                        "description": "label"
                      },
                      "sortindex": {
                        "type": "integer",
                        "format": "int32",
                        "description": "sortindex"
                      }
                    }
                  },
                  "description": "accounts"
                }
              }
            }
          }
        },
        "summary": "Get Available Customers",
        "operationId": "GetAvailableCustomers",
        "x-ms-visibility": "internal",
        "parameters": []
      }
    },
    "/connector/getUserServices": {
      "get": {
        "responses": {
          "200": {
            "description": "defaul1t",
            "schema": {
              "type": "object",
              "properties": {
                "services": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "type": {
                        "type": "string",
                        "description": "type"
                      },
                      "serviceFullname": {
                        "type": "string",
                        "description": "serviceFullname"
                      }
                    }
                  },
                  "description": "services"
                }
              }
            }
          }
        },
        "summary": "Get available services",
        "operationId": "GetAvailableServices",
        "parameters": [],
        "description": "test",
        "x-ms-visibility": "internal"
      }
    }
