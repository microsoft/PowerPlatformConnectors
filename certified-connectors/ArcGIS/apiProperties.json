{
  "properties": {
    "connectionParameters": {
      "token": {
        "type": "oauthSetting",
        "oAuthSettings": {
          "identityProvider": "oauth2",
          "clientId": "{{clientId}}",
          "scopes": [
            "user",
            "admin"
          ],
          "redirectMode": "Global",
          "redirectUrl": "https://global.consent.azure-apim.net/redirect",
          "properties": {
            "IsFirstParty": "False"
          },
          "customParameters": {
            "authorizationUrl": {
              "value": "https://www.arcgis.com/sharing/rest/oauth2/authorize"
            },
            "tokenUrl": {
              "value": "https://www.arcgis.com/sharing/rest/oauth2/token/"
            },
            "refreshUrl": {
              "value": "https://www.arcgis.com/sharing/rest/oauth2/token/"
            }
          }
        }
      }
    },
    "iconBrandColor": "#7B2CB0",
    "capabilities": [],
    "publisher": "Esri Inc.",
    "stackOwner": "Esri Inc."
