{
  "swagger": "2.0",
  "info": {
    "version": "v1",
    "title": "RamQuest Actions - UAT",
    "description": "This connector streamlines your RamQuest title production software solution processes, whether CCE or Horizon, by offering a suite of actions which can be performed from order retrieval and creation to document and task management."    ,
    "contact": {
      "name": "AE&I Support",
      "url": "https://www.ramquest.com/support",
      "email": "AEI@ortitletech.com"
    }
  }, 
  "host": "sm.uat.ortitletech.com",
  "basePath": "/pubcs-titleorder/v1",
  "schemes": [
    "https"
  ],
  "consumes": [],
  "produces": [],
  "paths": {
    "/order/{orderId}": {
      "get": {
        "tags": [
          "Order"
        ],
        "summary": "Get Order",
        "description": "Retrieve an existing RamQuest order",
        "operationId": "GetTitleOrder",
        "produces": [
          "text/plain",
          "application/json"      
        ],
        "parameters": [
          {
            "in": "path",
            "name": "orderId",
            "description": "Specify the File # to be retrieved",
            "required": true,
            "type": "string",
            "x-ms-summary": "File #",
            "x-ms-url-encoding": "single"
          }
        ],
        "responses": {
          "200": {
            "description": "Title-Order object",
            "schema": {
              "$ref": "#/definitions/PubCS.TitleOrder.Api.Models.TitleOrder"
            }
          }
        }
      }
    },
    "/order/{orderId}/note": {
      "put": {
        "consumes": [
          "application/json"
        ],
        "tags": [
          "Note"
        ],
        "summary": "Add Note",
        "description": "Add a note to an existing RamQuest order",
        "operationId": "AddNoteByOrder",
        "parameters": [
          {
            "x-ms-summary": "File #",
            "name": "orderId",
            "in": "path",
            "description": "Specify the File # to which to add the note",
            "required": true,
            "type": "string",
            "x-ms-url-encoding": "single"
          },
          {
            "x-ms-summary": "Note",
            "name": "note",
            "in": "body",
            "required": true,
            "description": "Note content",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "successfully created"
          }
        }
      }
    },
    "/orders/{orderId}/documents": {
      "post": {
        "tags": [
          "Document"
        ],
        "summary": "Add Document",
        "description": "Add a document to an existing RamQuest order",
        "operationId": "AddDocumentToOrder",
        "consumes": [
          "text/plain"
        ],
        "produces": [
          "text/plain"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "orderId",
            "x-ms-summary": "File #",
            "description": "Specify the File # to which to add the document",
            "required": true,
            "type": "string",
            "x-ms-url-encoding": "single"
          },
          {
            "in": "query",
            "name": "fileName",
            "x-ms-summary": "Document Name",
            "description": "Specify a name for the document including its file extension e.g. document_name.pdf",
            "type": "string",
            "required": true
          },
          {
            "in": "body",
            "name": "fileContent",
            "x-ms-summary": "Document Content",
            "description": "Document content from cloud storage connector (Microsoft OneDrive, Dropbox, etc.)",
            "schema": {
              "type": "string",
              "format": "binary"
            },
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
