                  "title": "House letter"
                },
                "floor": {
                  "type": "integer",
                  "format": "int32",
                  "description": "floor",
                  "title": "Floor"
                },
                "apartmentNo": {
                  "type": "string",
                  "description": "apartmentNo",
                  "title": "Apartment Number"
                },
                "gnr": {
                  "type": "integer",
                  "format": "int32",
                  "description": "gnr",
                  "title": "Land registration number"
                },
                "brnr": {
                  "type": "integer",
                  "format": "int32",
                  "description": "brnr",
                  "title": "Property registration number"
                },
                "zipCode": {
                  "type": "string",
                  "description": "zipCode",
                  "title": "Zip code"
                },
                "postalArea": {
                  "type": "string",
                  "description": "postalArea",
                  "title": "Postal area"
                },
                "phone": {
