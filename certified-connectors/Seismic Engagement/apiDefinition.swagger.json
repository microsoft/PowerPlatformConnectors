        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/Seismic.WorkSpaceContentManager.WsItemResp"
            }
          }
        }
      }
    },
    "/liveSend/links": {
      "post": {
        "tags": [
          "LiveSend"
        ],
        "summary": "Create a new LiveSend link",
        "description": "Creates a LiveSend link with the requested settings and content.",
        "operationId": "CreateLiveSendLink",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "body",
            "schema": {
              "$ref": "#/definitions/Seismic.LiveSend.LiveSendLink"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/Seismic.LiveSend.LiveSendLinkResponse"
            },
            "examples": {
              "application/json": {
                "url": "https://livesend.seismic.com/i/xxxxx",
                "id": "id",
                "settings": {
                  "expiresAt": "2022-12-09T12:53:07.5498113+05:30",
                  "password": "123456",
                  "allowDownload": true,
                  "notificationType": "All",
                  "singleView": false
                },
                "content": [
                  {
                    "id": "id",
                    "versionId": "versionId",
                    "teamsiteId": "1",
                    "repository": "DocCenter",
                    "name": "docxTest",
                    "type": "file",
                    "contentProfileId": "contentProfileId",
                    "contentProfilePath": [
                      ""
                    ]
                  },
                  {
                    "id": "id",
                    "versionId": "versionId",
                    "teamsiteId": "1",
                    "repository": "WorkSpace",
                    "name": "pptxTest",
                    "type": "file"
                  },
                  {
                    "id": "id",
                    "versionId": "versionId",
                    "teamsiteId": "1",
                    "repository": "WorkSpace",
                    "name": "pdfTest",
                    "type": "file"
                  }
                ]
              }
            }
          }
        }
      }
    },
    "/liveSend/settings": {
      "get": {
        "tags": [
          "LiveSend"
        ],
        "summary": "Get LiveSend Settings",
        "description": "Get LiveSend Settings",
        "operationId": "GetLiveSendSettings",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/Seismic.LiveSend.LiveSendSettingsResponse"
            },
            "examples": {
              "application/json": {
                "defaultExpirationDays": 20,
                "maxExpirationDays": 30,
                "allowDownload": true,
                "singleView": false,
                "passwordSettings": {
                  "requiresLowerCase": true,
                  "requiresNumber": true,
                  "requiresSymbols": true,
                  "requiresUpperCase": true,
                  "minimumLength": 8,
                  "isRequired": false
                }
              }
            }
          }
        }
      }
    }
  },
