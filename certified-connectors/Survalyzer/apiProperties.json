{
  "properties": {
    "connectionParameters": {
      "token": {
        "type": "oauthSetting",
        "oAuthSettings": {
          "identityProvider": "oauth2",
          "scopes": [
            "openid"
          ],
          "redirectMode": "Global",
          "redirectUrl": "https://global.consent.azure-apim.net/redirect",
          "properties": {
            "IsFirstParty": "False",
            "IsOnbehalfofLoginSupported": false
          },
          "customParameters": {
            "authorizationUrl": {
              "value": "https://authentication.survalyzer-@environmentVariables(\"sula_DataCenter\").app/openid/authorize"
            },
            "tokenUrl": {
              "value": "https://authentication.survalyzer-@environmentVariables(\"sula_DataCenter\").app/openid/token"
            },
            "refreshUrl": {
              "value": "https://authentication.survalyzer-@environmentVariables(\"sula_DataCenter\").app/openid/token"
            }
          },
          "clientId": "@environmentVariables(\"sula_Tenant\")_api",
          "clientSecret": "@environmentVariables(\"sula_ClientSecret\")"
        }
      }
    },
    "iconBrandColor": "#004b71",
    "capabilities": [
      "actions"
    ],
    "publisher": "Survalyzer AG",
    "stackOwner": "Survalyzer AG"
  }
}