        "responses": {
          "200": {
            "description": "Default response.",
            "schema": {
              "type": "object",
              "properties": {
                "executionId": {
                  "type": "string",
                  "description": "A unique ID for this execution."
                },
                "status": {
                  "type": "string",
                  "description": "A status indicator for this execution."
                }
              }
            }
          }
        }
      },
      "get": {
        "summary": "Get my workflows",
        "operationId": "GetWorkflows",
        "description": "Get a list of available workflows.",
        "x-ms-visibility": "internal",
        "consumes": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK.",
            "schema": {
              "type": "object",
              "properties": {
                "workflows": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "workflowId": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      }
                    }
                  }
                },
                "nextToken": {
                  "type": "string"
                }
              }
            }
          },
          "default": {
            "description": "Default response."
          }
        }
      }
    }
