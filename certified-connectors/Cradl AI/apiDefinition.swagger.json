    "/predictions": {
      "post": {
        "summary": "Parse document",
        "operationId": "ParseDocument",
        "description": "Extract data from documents like invoices, receipts, order confirmations, etc.",
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "Request",
            "schema": {
              "type": "object",
              "properties": {
                "modelId": {
                  "x-ms-summary": "Model",
                  "description": "Choose your model",
                  "type": "string",
                  "x-ms-dynamic-values": {
                    "operationId": "GetModels",
                    "value-path": "modelId",
                    "value-title": "name",
                    "value-collection": "models",
                    "parameters": {}
                  },
                  "x-ms-dynamic-list": {
                    "operationId": "GetModels",
                    "itemsPath": "models",
                    "itemValuePath": "modelId",
                    "itemTitlePath": "name",
                    "parameters": {}
                  },
                  "title": "Model",
                  "x-ms-visibility": "important"
                },
                "documentId": {
                  "x-ms-summary": "Document ID",
                  "description": "Name of the file, e.g. Document.pdf.",
                  "type": "string",
                  "title": "Document ID",
                  "x-ms-visibility": "important"
                },
                "postprocessConfig": {
                  "$ref": "#/definitions/PostprocessConfig"
                },
                "preprocessConfig": {
                  "$ref": "#/definitions/PreprocessConfig"
                }
              },
              "x-ms-visibility": "important",
              "required": [
                "documentId",
                "modelId"
              ]
            },
            "x-ms-visibility": "important",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Default response.",
            "schema": {
              "type": "object",
              "properties": {
                "documentId": {
                  "type": "string",
                  "description": "The document which was parsed."
                },
                "inferenceTime": {
                  "type": "number",
                  "format": "float",
                  "description": "The number of seconds used to process this document."
                },
                "modelId": {
                  "type": "string",
                  "description": "The model which was used."
                },
                "postprocessConfig": {
                  "$ref": "#/definitions/PostprocessConfig"
                },
                "preprocessConfig": {
                  "$ref": "#/definitions/PreprocessConfig"
                },
                "predictions": {
                  "type": "object",
                  "description": "predictions",
                  "additionalProperties": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "label": {
                          "type": "string"
                        },
                        "value": {
                          "type": "string"
                        },
                        "confidence": {
                          "type": "number",
                          "format": "float"
                        }
                      }
                    }
                  }
                },
                "trainingId": {
                  "type": "string",
                  "description": "The model training."
                }
              }
            }
          }
        }
      }
