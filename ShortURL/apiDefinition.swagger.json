    "version": "v1",
    "x-ms-api-annotation": {
      "status": "Preview"
    },
    "contact":{
      "name":"APPS 365 LTD",
      "url":"https://apps365ltd.com/support",
      "email":"support@apps365ltd.com"
    } 
  },
