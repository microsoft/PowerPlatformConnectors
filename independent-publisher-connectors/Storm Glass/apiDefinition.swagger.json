                              }
                            },
                            "description": "Objects describing the closest moon phase"
                          },
                          "current": {
                            "type": "object",
                            "properties": {
                              "text": {
                                "type": "string",
                                "description": "A string describing the moon phase"
                              },
                              "time": {
                                "type": "string",
                                "description": "Timestamp in UTC showing what time the moon phase object describes"
                              },
                              "value": {
                                "type": "number",
                                "format": "float",
                                "description": "A float value for the phase of the given time."
                              }
                            },
                            "description": "Objects describing the current moon phase"
                          }
                        },
                        "description": "Moon Phase"
                      },
                      "moonrise": {
                        "type": "string",
                        "description": "Moon Rise"
                      },
                      "moonset": {
                        "type": "string",
                        "description": "Moon Set"
                      },
                      "nauticalDawn": {
                        "type": "string",
                        "description": "Nautical Dawn"
                      },
                      "nauticalDusk": {
                        "type": "string",
                        "description": "Nautical Dusk"
                      },
                      "sunrise": {
                        "type": "string",
                        "description": "Sunrise"
                      },
                      "sunset": {
                        "type": "string",
                        "description": "Sunset"
                      },
                      "time": {
                        "type": "string",
                        "description": "Time"
                      }
                    }
                  },
                  "description": "data"
                },
                "meta": {
                  "type": "object",
                  "properties": {
                    "cost": {
                      "type": "integer",
                      "format": "int32",
                      "description": "Cost"
                    },
                    "dailyQuota": {
                      "type": "integer",
                      "format": "int32",
                      "description": "Daily Quota"
                    },
                    "lat": {
                      "type": "number",
                      "format": "float",
                      "description": "Latitude"
                    },
                    "lng": {
                      "type": "number",
                      "format": "float",
                      "description": "Longitude"
                    },
                    "requestCount": {
                      "type": "integer",
                      "format": "int32",
                      "description": "Request Count"
                    },
                    "start": {
                      "type": "string",
                      "description": "Start Time"
                    }
                  },
                  "description": "The meta object contains information about the API request"
                }
              }
            }
          }
        },
        "summary": "Astronomy Point Request",
        "description": "Retrieve sunrise, sunset, moonrise, moonset and moon phase for a single coordinate.",
        "operationId": "AstronomyPointRequest",
        "parameters": [
          {
            "$ref": "#/parameters/latReq"
          },
          {
            "$ref": "#/parameters/lngReq"
          },
          {
            "$ref": "#/parameters/end"
          },
          {
            "$ref": "#/parameters/start"
          }
        ]
      }
    },
