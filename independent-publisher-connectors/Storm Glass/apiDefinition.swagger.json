                      "astronomicalDusk": {
                        "type": "string",
                        "description": "Astronomical Dusk"
                      },
                      "civilDawn": {
                        "type": "string",
                        "description": "Civil Dawn"
                      },
                      "civilDusk": {
                        "type": "string",
                        "description": "Civil Dusk"
                      },
                      "moonFraction": {
                        "type": "number",
                        "format": "float",
