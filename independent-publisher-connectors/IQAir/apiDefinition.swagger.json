  "definitions": {
    "CityResponse": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string",
          "description": "Status of the response.",
          "title": "Status"
        },
        "data": {
          "type": "object",
          "properties": {
            "city": {
              "type": "string",
              "description": "Name of the city.",
              "title": "City"
            },
            "state": {
              "type": "string",
              "description": "Name of the state including the city.",
              "title": "State"
            },
            "country": {
              "type": "string",
              "description": "Name of the country including the city.",
              "title": "Country"
            },
            "location": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "description": "Type of coordinates, e.g.: point.",
                  "title": "Type"
                },
                "coordinates": {
                  "type": "array",
                  "items": {
                    "type": "number",
                    "format": "float"
                  },
                  "description": "Array of coordinates: longitude and latitude.",
                  "title": "Coordinates"
                }
              },
              "description": "Location information.",
              "title": "Location"
            },
            "current": {
              "type": "object",
              "properties": {
                "pollution": {
                  "type": "object",
                  "properties": {
                    "ts": {
                      "type": "string",
                      "description": "Timestamp of results.",
                      "title": "Timestamp"
                    },
                    "aqius": {
                      "type": "integer",
                      "format": "int32",
                      "description": "Air Quality Index (US)",
                      "title": "AQI-US"
                    },
                    "mainus": {
                      "type": "string",
                      "description": "Main pollutant (US), for example PM2.5",
                      "title": "Main Pollutant - US"
                    },
                    "aqicn": {
                      "type": "integer",
                      "format": "int32",
                      "description": "Air Quality Index (CN)",
                      "title": "AQI-CN"
                    },
                    "maincn": {
                      "type": "string",
                      "description": "Main pollutant (CN), for example PM10",
                      "title": "Main Pollutant - CN"
                    }
                  },
                  "description": "Pollution information.",
                  "title": "Pollution"
                },
                "weather": {
                  "type": "object",
                  "properties": {
                    "ts": {
                      "type": "string",
                      "description": "Timestamp of the results.",
                      "title": "Timestamp"
                    },
                    "tp": {
                      "type": "integer",
                      "format": "int32",
                      "description": "Temperature, expressed in Celsius.",
                      "title": "Temperature"
                    },
                    "pr": {
                      "type": "integer",
                      "format": "int32",
                      "description": "Air pressure, expressed in hPa or mbar.",
                      "title": "Air Pressure"
                    },
                    "hu": {
                      "type": "integer",
                      "format": "int32",
                      "description": "Humidity, percentage (%).",
                      "title": "Humidity"
                    },
                    "ws": {
                      "type": "number",
                      "format": "float",
                      "description": "Wind speed.",
                      "title": "Wind speed"
                    },
                    "wd": {
                      "type": "integer",
                      "format": "int32",
                      "description": "Wind direction, in degrees.",
                      "title": "Wind direction"
                    },
                    "ic": {
                      "type": "string",
                      "description": "Weather icon identifier.",
                      "title": "Icon"
                    }
                  },
                  "description": "Weather information.",
                  "title": "Weather"
                }
              },
              "description": "Current dataset.",
              "title": "Current"
            }
          },
          "description": "Pollution and weather information.",
          "title": "Data"
        }
