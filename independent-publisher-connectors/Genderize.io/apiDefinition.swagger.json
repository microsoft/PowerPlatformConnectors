  "swagger": "2.0",
  "info": {
    "title": "Genderize_io",
    "description": "A simple API to predict the gender of a person given their name.\nThe API is free for up to 1000 names/day.",
    "version": "1.0",
    "contact": {
      "name": "Tomasz Poszytek",
      "url": "https://aka.ms/poszytek",
      "email": "tomasz@poszytek.eu"
    }
  },
