  "host": "api.genderize.io",
  "basePath": "/",
  "schemes": [
    "https"
  ],
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "paths": {
    "/": {
      "get": {
        "responses": {
          "200": {
            "description": "200",
            "schema": {
              "type": "array",
              "items": {
                "description": "List of checks.",
                "x-ms-visibility": "advanced",
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "description": "Checked name.",
                    "title": "Name",
                    "x-ms-visibility": "important"
                  },
                  "gender": {
                    "type": "string",
                    "description": "Predicted gender for the given name.",
                    "title": "Gender",
                    "x-ms-visibility": "important"
                  },
                  "probability": {
                    "type": "number",
                    "format": "float",
                    "description": "The certainty of the assigned gender. Basically the ratio of male to females.",
                    "title": "Probability",
                    "x-ms-visibility": "important"
                  },
                  "count": {
                    "type": "integer",
                    "format": "int32",
                    "description": "Number of data rows examined in order to calculate the response.",
                    "title": "Count",
                    "x-ms-visibility": "important"
                  }
                }
              }
            },
            "headers": {
              "X-Rate-Limit-Limit": {
                "description": "The amount of names available in the current time window",
                "type": "integer",
                "x-ms-visibility": "advanced"
              },
              "X-Rate-Reset": {
                "description": "The number of names left in the current time window ",
                "type": "integer",
                "x-ms-visibility": "advanced"
              },
              "X-Rate-Limit-Remaining": {
                "description": "Seconds remaining until a new time window opens",
                "type": "integer",
                "x-ms-visibility": "advanced"
              }
            }
          }
        },
        "summary": "Check name(s) gender",
        "description": "Check gender of a single name or multiple names. For multiple names - separate them with comma. Up to 10 names at a time.",
        "operationId": "CheckNamesGender",
        "parameters": [
          {
