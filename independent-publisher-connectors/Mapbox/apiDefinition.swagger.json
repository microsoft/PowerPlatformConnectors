                      },
                      "weight": {
                        "type": "number",
                        "format": "float",
                        "description": "The weight in unit described in weight_name",
                        "x-ms-visibility": "internal"
                      },
                      "duration": {
                        "type": "number",
                        "format": "float",
                        "description": "The estimated travel time through the waypoints in seconds",
                        "x-ms-visibility": "important"
                      },
