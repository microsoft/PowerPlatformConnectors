                                              },
                                              "description": "secondary components",
                                              "x-ms-visibility": "internal"
                                            },
                                            "type": {
                                              "type": "string",
                                              "description": "type",
                                              "x-ms-visibility": "internal"
                                            },
                                            "modifier": {
                                              "type": "string",
                                              "description": "modifier",
