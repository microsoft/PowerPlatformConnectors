                                            "modifier": {
                                              "type": "string",
                                              "description": "The modifier for the maneuver. Can be used in combination with the type (and, if it is a roundabout, the degrees) for an icon to display.",
                                              "x-ms-visibility": "internal"
                                            }
                                          },
                                          "description": "The most important content to display to the user. This text is larger and at the top.",
                                          "x-ms-visibility": "internal"
                                        },
                                        "secondary": {
                                          "type": "object",
                                          "properties": {
                                            "text": {
                                              "type": "string",
                                              "description": "text",
                                              "x-ms-visibility": "internal"
                                            },
                                            "components": {
                                              "type": "array",
                                              "items": {
                                                "type": "object",
                                                "properties": {
                                                  "text": {
                                                    "type": "string",
                                                    "description": "text",
                                                    "x-ms-visibility": "internal"
