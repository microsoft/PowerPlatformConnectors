                                          "x-ms-visibility": "internal"
                                        },
                                        "bearings": {
                                          "type": "array",
                                          "items": {
                                            "type": "integer",
                                            "format": "int32",
                                            "title": "Bearing item",
                                            "x-ms-visibility": "internal"
                                          },
                                          "description": "A list of bearing values that are available at the intersection. The bearings describe all available roads at the intersection.",
                                          "x-ms-visibility": "internal"
                                        },
                                        "location": {
                                          "type": "array",
                                          "items": {
                                            "type": "number",
                                            "format": "float",
                                            "title": "Intersection location item",
                                            "x-ms-visibility": "internal"
                                          },
                                          "description": "A [longitude, latitude] pair describing the location of the turn.",
                                          "x-ms-visibility": "internal"
                                        },
                                        "in": {
                                          "type": "integer",
                                          "format": "int32",
                                          "description": "The index in the bearings and entry arrays. Used to calculate the bearing before the turn",
                                          "x-ms-visibility": "internal"
                                        }
                                      },
                                      "description": "Single intersection in the step",
                                      "title": "Single intersection item",
                                      "x-ms-visibility": "internal"
                                    },
                                    "description": "An array of objects representing all the intersections along the step",
                                    "x-ms-visibility": "internal"
                                  },
                                  "driving_side": {
                                    "type": "string",
                                    "description": "The legal driving side at the location for this step. Either left or right.",
                                    "x-ms-visibility": "advanced"
                                  },
                                  "geometry": {
                                    "type": "string",
                                    "description": "Depending on the geometries parameter, this is a GeoJSON LineString or a Polyline string representing the full route geometry from this route step to the next route step",
                                    "x-ms-visibility": "internal"
                                  },
                                  "mode": {
                                    "type": "string",
                                    "description": "The mode of transportation",
                                    "x-ms-visibility": "advanced"
                                  },
