                                        "type": "integer",
                                        "format": "int32",
                                        "description": "A number between 0 and 360 indicating the clockwise angle from true north to the direction of travel immediately after the maneuve"
                                      },
                                      "bearing_before": {
                                        "type": "integer",
                                        "format": "int32",
                                        "description": "A number between 0 and 360 indicating the clockwise angle from true north to the direction of travel immediately before the maneuve",
                                        "x-ms-visibility": "internal"
                                      },
                                      "location": {
                                        "type": "array",
                                        "items": {
                                          "type": "number",
                                          "format": "float",
                                          "description": "a coordinate of the maneuver",
                                          "x-ms-visibility": "internal"
                                        },
                                        "description": "The coordinates of the maneuver as [longitude, latitude]",
                                        "title": "maneuver_location",
                                        "x-ms-visibility": "internal"
                                      },
                                      "modifier": {
                                        "type": "string",
                                        "description": "The direction change of the maneuver",
                                        "title": "maneuver_modifier",
                                        "x-ms-visibility": "internal"
                                      },
                                      "type": {
                                        "type": "string",
