                                    "type": "array",
                                    "items": {
                                      "type": "object",
                                      "properties": {
                                        "distanceAlongGeometry": {
                                          "type": "number",
                                          "format": "float",
                                          "description": "distanceAlongGeometry",
                                          "x-ms-visibility": "internal"
                                        },
                                        "announcement": {
                                          "type": "string",
                                          "description": "announcement",
                                          "x-ms-visibility": "internal"
                                        },
                                        "ssmlAnnouncement": {
                                          "type": "string",
                                          "description": "ssmlAnnouncement",
                                          "x-ms-visibility": "internal"
                                        }
                                      },
                                      "x-ms-visibility": "internal",
                                      "title": "voiceInstruction_ite"
                                    },
                                    "description": "voiceInstructions",
                                    "x-ms-visibility": "internal"
                                  },
                                  "bannerInstructions": {
                                    "type": "array",
                                    "items": {
                                      "type": "object",
                                      "properties": {
                                        "distanceAlongGeometry": {
                                          "type": "integer",
                                          "format": "int32",
                                          "description": "A float indicating how far from the upcoming maneuver the banner instruction should begin being displayed, in meters",
                                          "x-ms-visibility": "internal"
                                        },
                                        "primary": {
                                          "type": "object",
                                          "properties": {
                                            "text": {
                                              "type": "string",
                                              "description": "All text that should be displayed",
                                              "x-ms-visibility": "internal"
                                            },
                                            "components": {
                                              "type": "array",
                                              "items": {
                                                "type": "object",
                                                "properties": {
                                                  "text": {
                                                    "type": "string",
                                                    "description": "The sub-string of the text of the parent objects that may have additional context associated with it",
                                                    "x-ms-visibility": "internal"
                                                  }
                                                },
                                                "title": "components array item",
                                                "x-ms-visibility": "internal"
                                              },
                                              "description": "An array of objects that, together, make up what should be displayed in the banner. Includes additional information intended to be used to aid in visual layout. A component can contain the following properties:",
                                              "x-ms-visibility": "internal"
                                            },
                                            "type": {
                                              "type": "string",
                                              "description": "The type of maneuver. May be used in combination with the modifier (and, if it is a roundabout, the degrees) for an icon to display",
                                              "x-ms-visibility": "internal"
