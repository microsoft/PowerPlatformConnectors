{
  "swagger": "2.0",
  "info": {
    "title": "Mapbox",
    "description": "Mapbox allows you to access its navigation, location search and static map generation services to create interative/static maps in your application.",
    "version": "1.0",
    "contact": {
      "name": "Simone Lin",
      "email": "simonelinrj@outlook.com"

    }
  },
  "host": "api.mapbox.com",
  "basePath": "/",
  "schemes": [
    "https"
  ],
  "consumes": [],
  "produces": [],
  "paths": {
    "/geocoding/v5/mapbox.places/{search_text}.json": {
      "get": {
        "responses": {
          "200": {
            "description": "default",
            "schema": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "description": "type",
                  "title": "properties_type",
                  "x-ms-visibility": "internal"
                },
                "query": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "description": "search text query words",
                    "title": "search text word"
                  },
                  "description": "An array of space and punctuation separated strings from the original query"
                },
                "features": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "description": "feature ID in the format type.id",
                        "title": "feature_id",
                        "x-ms-visibility": "internal"
                      },
                      "type": {
                        "type": "string",
                        "description": "a GeoJSON type",
                        "title": "feature_type",
                        "x-ms-visibility": "internal"
                      },
                      "place_type": {
                        "type": "array",
                        "items": {
                          "type": "string",
                          "x-ms-visibility": "internal"
                        },
                        "description": "An array of feature types describing the feature",
                        "x-ms-visibility": "internal"
                      },
                      "relevance": {
                        "type": "number",
                        "format": "float",
                        "description": "Indicates how well the returned feature matches to user's query on a scale of 0 to 1",
                        "title": "feature_relevance",
                        "x-ms-visibility": "important"
                      },
                      "properties": {
                        "type": "object",
                        "properties": {},
                        "description": "An object describing the feature",
                        "x-ms-visibility": "internal"
                      },
                      "text": {
                        "type": "string",
                        "description": "A string representing the feature in the requested language if specified",
                        "title": "feature_text",
                        "x-ms-visibility": "internal"
                      },
                      "place_name": {
                        "type": "string",
                        "description": "A string representing the feature in the requested language if specified and its full result hierarchy",
                        "title": "feature_placename",
                        "x-ms-visibility": "advanced"
                      },
                      "matching_text": {
                        "type": "string",
                        "description": "Optional. A string analogous to the text field that more closely matches the query than results in the specified language",
                        "title": "feature_matchingtext",
                        "x-ms-visibility": "internal"
                      },
                      "matching_place_name": {
                        "type": "string",
                        "description": "Optional. A string analogous to the place_name field that more closely matches the query than results in the specified language",
                        "title": "feature_matchingplacename",
                        "x-ms-visibility": "internal"
                      },
                      "bbox": {
                        "type": "array",
                        "items": {
                          "type": "number",
                          "format": "float",
                          "description": "minX, minY, maxX, maxY of the bounding box"
                        },
                        "description": "A bounding box array",
                        "x-ms-visibility": "advanced"
                      },
                      "center": {
                        "type": "array",
                        "items": {
                          "type": "number",
                          "format": "float",
                          "description": "longitude and latitude of the center"
                        },
                        "description": "The coordinates of the feature's center in the form of [longitude, latitude], may be the literal center or the center of human activities",
                        "x-ms-visibility": "important"
                      },
                      "geometry": {
                        "type": "object",
                        "properties": {
                          "type": {
                            "type": "string",
                            "description": "type",
                            "x-ms-visibility": "internal"
                          },
                          "coordinates": {
                            "type": "array",
                            "items": {
                              "type": "number",
                              "format": "float"
                            },
                            "description": "an array in the format [longitude, latitude] at the center of the specified bounding box"
                          },
                          "interpolated": {
                            "type": "boolean",
                            "description": "interpolated",
                            "x-ms-visibility": "internal",
                            "enum": [
                              true,
                              false
                            ]
                          },
                          "omitted": {
                            "type": "boolean",
                            "description": "omitted",
                            "x-ms-visibility": "internal",
                            "enum": [
                              true,
                              false
                            ]
                          }
                        },
                        "description": "an object describing the spatial geometry of the returned feature",
                        "x-ms-visibility": "internal"
                      },
                      "address": {
                        "type": "string",
                        "description": "The house number of the returned address feature",
                        "x-ms-visibility": "internal"
                      },
                      "context": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "id": {
                              "type": "string",
                              "description": "id"
                            },
                            "text": {
                              "type": "string",
                              "description": "text"
                            },
                            "wikidata": {
                              "type": "string",
                              "description": "wikidata"
                            },
                            "short_code": {
                              "type": "string",
                              "description": "short_code"
                            }
                          }
                        },
                        "description": "An array representing the hierarchy of encompassing parent features",
                        "x-ms-visibility": "internal"
                      }
                    }
                  },
                  "description": "An array of feature objects ordered by relevance"
                },
                "attribution": {
                  "type": "string",
                  "description": "attributes the result to Mapbox",
                  "x-ms-visibility": "internal"
                }
              }
            }
          }
        },
        "summary": "Forward Geocoding",
        "description": "This action allows you to look up a single location by name and will return you the geographic coordinates (latitude and longitude) of the location",
        "operationId": "FwGeocoding",
        "parameters": [
          {
            "name": "search_text",
            "in": "path",
            "required": true,
            "type": "string",
            "x-ms-visibility": "important",
            "x-ms-summary": "Address or post code of the location.",
            "description": "An address or a post code of the location that the user wants to retrieve geocoordinates from."
          },
          {
            "name": "proximity",
            "in": "query",
            "required": false,
            "type": "string",
            "default": "ip",
            "description":"Bias the response to favor results that are closer to this location. Default is ip, or can be longitude,latitude coordinates",
            "x-ms-summary": "Favor results taht are closer to the specified location",
            "x-ms-visibility": "advanced"
          },
          {
            "name": "types",
            "in": "query",
            "required": false,
            "type": "string",
            "default": "place,postcode,address",
            "description":"Filter results to include only a subset (one or more) of the available feature types. Options are country, region, postcode, district, place, locality, neighborhood, address, and poi",
            "x-ms-summary": "Feature types included in the search",
            "x-ms-visibility": "advanced"
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "type": "integer",
            "default": 2,
            "description": "Specify the maximum number of results to return, default is 2 and maximum is 10",
            "x-ms-summary": "Maximum number of results to return",
            "x-ms-visibility": "advanced"
          },
          {
            "name": "autocomplete",
            "in": "query",
            "required": false,
            "type": "boolean",
            "default": false,
            "description": "Specify whether to return autocomplete results, default is false.",
            "x-ms-summary": "Set autocomplete results",
            "x-ms-visibility": "advanced"
          },
          {
            "name": "language",
            "in": "query",
            "required": false,
            "type": "string",
            "default": "en",
            "description":"Specify the user's language. This parameter controls the language of the text supplied in responses.",
            "x-ms-summary": "Specify the user's language",
            "x-ms-visibility": "advanced"
          }
        ],
        "x-ms-visibility": "important"
      }
    },
    "/styles/v1/mapbox/streets-v11/static/pin-s+ff2211({pinlon},{pinlat})/{lon},{lat},{zoom},{bearing},{pitch}/{width}x{height}": {
      "get": {
        "responses": {
          "200": {
            "description": "default",
            "schema": {
              "type": "string",
              "format": "binary",
              "x-ms-media-kind": "image"
            }
          }
        },
        "summary": "Get Static Map Image",
        "description": "This action outputs a static map image of a location in base64, the location is specified by its longitude and latitude. The static map image does not have interactivity or controls.",
        "operationId": "GetStaticMap",
        "x-ms-visibility": "important",
        "parameters": [
          {
            "name": "pinlon",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "longitude value which should be the same as the center longitude specified in variable lon, in order to pin the desired location",
            "x-ms-summary": "Pin longitude",
            "x-ms-visibility": "important"
          },
          {
            "name": "pinlat",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "latitude value which should be the same as the center latitude specified in variable lon, in order to pin the desired location",
            "x-ms-summary": "Pin latitude",
            "x-ms-visibility": "important"
          },
          {
            "name": "lon",
            "in": "path",
            "required": true,
            "type": "number",
            "description": "Longitude for the center point of the static map; a number between -180 and 180.",
            "x-ms-summary": "Longitude",
            "x-ms-visibility": "important",
            "format": "float"
          },
          {
            "name": "lat",
            "in": "path",
            "required": true,
            "type": "number",
            "description": "Latitude for the center point of the static map; a number between -85.0511 and 85.0511.",
            "x-ms-summary": "Latitude",
            "x-ms-visibility": "important",
            "format": "float"
          },
          {
            "name": "zoom",
            "in": "path",
            "required": true,
            "type": "number",
            "default": 13.31,
            "description": "Zoom level; a number between 0 and 22. Fractional zoom levels will be rounded to two decimal places",
            "x-ms-summary": "Image Zoom",
            "format": "float",
            "x-ms-visibility": "important"
          },
          {
            "name": "bearing",
            "in": "path",
            "required": true,
            "type": "number",
            "default": 0,
            "description": "Bearing rotates the map around its center. A number between 0 and 360, interpreted as decimal degrees.",
            "x-ms-summary": "Image Rotation",
            "x-ms-visibility": "advanced",
            "format": "float"
          },
          {
            "name": "pitch",
            "in": "path",
            "required": true,
            "type": "number",
            "default": 0,
            "description": "Pitch tilts the map, producing a perspective effect. A number between 0 and 60, measured in degrees.",
            "x-ms-summary": "Image Tilt",
            "x-ms-visibility": "advanced",
            "format": "float"
          },
          {
            "name": "width",
            "in": "path",
            "required": true,
            "type": "integer",
            "default": 300,
            "description": "Width of the image; a number between 1 and 1280 pixels.",
            "x-ms-summary": "Image Width",
            "x-ms-visibility": "important",
            "format": "int32"
          },
          {
            "name": "height",
            "in": "path",
            "required": true,
            "type": "integer",
            "default": 200,
            "description": "Height of the image; a number between 1 and 1280 pixels.",
            "x-ms-summary": "Image Height",
            "x-ms-visibility": "important",
            "format": "int32"
          }
        ]
      }
    },
    "/directions/v5/mapbox/{routing}/{coordinates}": {
      "get": {
        "responses": {
          "200": {
            "description": "default",
            "schema": {
              "type": "object",
              "properties": {
                "routes": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "geometry": {
                        "type": "string",
                        "description": "Depending on the geometries query parameter it can be either GeoJSON LineString or Polyline string, depending on the overview it can either be full or simplified",
                        "x-ms-visibility": "internal"
                      },
                      "legs": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "summary": {
                              "type": "string",
                              "description": "A summary of the route",
                              "title": "Route summary",
                              "x-ms-visibility": "important"
                            },
                            "weight": {
                              "type": "number",
                              "format": "float",
                              "description": "The weight in units",
                              "x-ms-visibility": "internal"
                            },
                            "duration": {
                              "type": "number",
                              "format": "float",
                              "description": "The estimated travel time beteen route legs waypoints in seconds",
                              "title": "Route leg duration",
                              "x-ms-visibility": "important"
                            },
