        "parameters": [
          {
            "name": "energy",
            "in": "query",
            "x-ms-summary": "Energy",
            "description": "The energy type.",
            "required": true,
            "type": "string",
            "enum": [
              "Solar",
              "Wind",
              "HydroElectric",
              "Biomass",
              "Geothermal",
              "Tidal",
              "OtherCleanEnergy"
            ]
          },
          {
            "name": "consumption",
            "in": "query",
            "x-ms-summary": "Consumption",
            "description": "The consumption amount.",
            "required": true,
            "type": "string"
          }
        ]
      }
    },
    "/FuelToCO2e": {
