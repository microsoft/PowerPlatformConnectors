              "en-US",
              "pt-BR",
              "es-ES",
              "ca-ES",
              "de-DE",
              "it-IT",
              "fr-FR",
              "sv-SE",
              "id-ID",
              "pl-PL",
              "ja-JP",
              "zh-TW",
              "zh-CN",
              "ko-KR",
              "th-TH",
              "nl-NL",
              "hu-HU",
              "vi-VN",
              "cs-CZ",
              "da-DK",
              "fi-FI",
              "uk-UA",
              "el-GR",
              "ro-RO",
              "nb-NO",
              "sk-SK",
              "tr-TR",
              "ru-RU"
            ]
          },
          {
            "name": "page",
            "in": "query",
            "required": false,
            "type": "integer",
            "default": 1,
            "description": "The page number you are requesting.",
            "x-ms-summary": "Page Number"
          },
          {
            "name": "per_page",
            "in": "query",
            "required": false,
            "type": "integer",
            "description": "The number of results you are requesting per page. Default value is 15 and Max limit is 80",
            "x-ms-summary": "Per Page"
          }
        ]
