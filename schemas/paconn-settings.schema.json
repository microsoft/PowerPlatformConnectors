{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "id": "https://github.com/microsoft/PowerPlatformConnectors/blob/master/schemas/paconn-settings.schema.json#",
  "type": "object",
  "required": [],
  "additionalProperties": false,
  "properties": {
    "connectorId": {
      "type": "string"
    },
    "environment": {
      "type": "string"
    },
    "apiProperties": {
      "type": "string",
      "default": "apiProperties.json"
    },
    "apiDefinition": {
      "type": "string",
      "default": "apiDefinition.swagger.json"
    },
    "icon": {
      "type": "string",
      "default": "icon.png"
    },
    "powerAppsUrl": {
      "type": "string",
      "format": "uri",
      "default": "https://api.powerapps.com",
      "defaultSnippets": [
        {
          "label": "PROD",
          "body": "https://api.powerapps.com"
        },
        {
          "label": "TIP1",
          "body": "https://tip1.api.powerapps.com"
        }
      ]
    },
    "powerAppsApiVersion": {
      "type": "string",
      "default": "2016-11-01",
      "enum": [
        "2016-11-01"
      ]
    },
    "flowUrl": {
      "type": "string",
      "format": "uri",
      "default": "https://api.flow.microsoft.com",
      "defaultSnippets": [
        {
          "label": "PROD",
          "body": "https://api.flow.microsoft.com"
        },
        {
          "label": "TIP1",
          "body": "https://tip1.api.flow.microsoft.com"
        }
      ]
    },
    "flowApiVersion": {
      "type": "string",
      "default": "2016-11-01",
      "enum": [
        "2016-11-01"
      ]
    }
  },
  "definitions": {}
}